@model SaafiMoney.Models.SenderViewModels.SenderIndexViewModel
<h1>Senders</h1>

<table class ="table table-hover">
    <tbody>
        @foreach(var sender in Model.SenderList)
        {
            <tr>
                <td>@sender.FirstName</td>
                <td>@sender.LastName</td>
                
            </tr>

        }
    </tbody>
</table>
